    "        segmentation_success = False\n",
    "\nelse:\n",
    "    print(\"‚ö†Ô∏è Skipping segmentation - prerequisites not met\")\n",
    "    if not cellpose_working:\n",
    "        print(\"   Reason: Cellpose not working\")\n",
    "    if not test_data_created:\n",
    "        print(\"   Reason: Test data not created\")\n",
    "    segmentation_success = False\n",
    "\n",
    "print(f\"\\nSegmentation Status: {'‚úÖ SUCCESS' if segmentation_success else '‚ùå FAILED'}\")"
