        "## Step 1: Quick Installation Check"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "import sys\n",
        "import os\n",
        "import subprocess\n",
        "\n",
        "print(f\"Python version: {sys.version}\")\n",
        "print(f\"Working directory: {os.getcwd()}\")\n",
        "\n",
        "# Simple command runner\n",
        "def run_cmd(cmd, timeout=180):\n",
        "    try:\n",
        "        result = subprocess.run(cmd, shell=True, capture_output=True, text=True, timeout=timeout)\n",
        "        return result.returncode == 0, result.stdout.strip(), result.stderr.strip()\n",
        "    except:\n",
        "        return False, \"\", \"Command failed\""
      ]
    },
    {
      "cell_type": "markdown",
      "metadata": {},
      "source": [
        "## Step 2: Install Cellpose (Simple Approach)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "print(\"Installing Cellpose and dependencies...\")\n",
        "\n",
        "# Install core packages\n",
        "packages = [\n",
        "    \"numpy\", \"scipy\", \"matplotlib\", \"scikit-image\", \n",
        "    \"opencv-python-headless\", \"tifffile\", \"torch\", \"cellpose\"\n",
        "]\n",
        "\n",
        "for pkg in packages:\n",
        "    print(f\"Installing {pkg}...\")\n",
        "    success, stdout, stderr = run_cmd(f\"pip install {pkg} --quiet\")\n",
        "    if success:\n",
        "        print(f\"  ‚úÖ {pkg} installed\")\n",
        "    else:\n",
        "        print(f\"  ‚ö†Ô∏è {pkg}: {stderr[:50]}...\")\n",
        "\n",
        "print(\"\\n‚úÖ Installation completed!\")"
      ]
    },
    {
